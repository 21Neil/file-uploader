<header>
  <h1><%= title %></h1>
  <div>
    <a href="/"><button>Home</button></a>
    <% if (currentUser) { %>
    <a href="/folder/<%= currentUser.folder[0].id %>"><button>My files</button></a>
    <a href="/logout"><button>Logout</button></a>
    <% } else { %>
    <a href="/login"><button>Login</button></a>
    <a href="/sign-up"><button>Sign up</button></a>
    <% } %>
  </div>
</header>
